page{
  background:#f9f9f9;
}
.tab-list{
  background:#fff;
  display:flex;
  justify-content:space-around;
  font-size:33rpx;
  color:#333;
  height:98rpx;
  line-height:98rpx;
  border-bottom:2rpx solid #e5e5e5;
}
.tab-list text.active{
  border-bottom:3px solid #16bf76;
}
.content-list{
  width:94%;
  margin:0 auto;
  padding-bottom:66rpx;
}
.content-list .list{
  width:100%;
  margin-top:30rpx;
}
.content-list image{
  width:100%;
  height:320rpx;
  border-radius:20rpx;
}
.content-list .text{
  font-size:30rpx;
  color:#333;
  font-weight:bold;
  line-height:68rpx;
}