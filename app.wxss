/*登录授权样式*/
.sLogin{
  font-size: 40rpx;
  color: #ccc;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.mask{
position:fixed;
background:rgba(0, 0, 0, 0.5);
top:0;
right:0;
left:0;
bottom:0;
z-index:1;
}
.model{
position:fixed;
top:50%;
left:50%;
transform: translate(-50%, -50%);
z-index:2;
background:#fff;
width:530rpx;
height:660rpx;
border-radius: 20rpx;
padding-top:78rpx;
}
.modelTitle{
width:100%;
height:100rpx;
line-height:100rpx;
font-size:32rpx;
text-align:center;
margin-bottom:10rpx;
color:#555;
}
.modelBody{
display: flex;
justify-content: center;
align-items: center;
}
.modelBody .img{
width: 60rpx;
height: 60rpx;
margin-right: 20rpx;
}
.modelBody image{
width: 60rpx;
height: 60rpx; 
}
.modelBody .tips view:first-child{
font-size: 24rpx;
color: #666666;
line-height: 24rpx;
margin-bottom: 10rpx;
text-align: center;
}
.modelBody .tips view:last-child{
font-size: 22rpx;
color: #999999;
line-height: 22rpx;
text-align: center;
}
.icon-group{
text-align: center;
}
.icon-group image{
width: 270rpx;
height: 270rpx;
}
.agree{
font-size: 32rpx;
color: #ffffff;
line-height: 80rpx;
width:440rpx;
height: 80rpx;
margin-top:57rpx !important;
margin-bottom:27rpx !important;
}


